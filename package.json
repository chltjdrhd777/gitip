{
  "name": "gitip",
  "version": "1.0.0",
  "description": "auto generate git issue and pull request",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node ./src/index.ts",
    "link": "rm -rf dist && npm run build && npm link",
    "check:type": "tsc --noEmit",
    "check:lint": "npx eslint . --quiet --ignore-pattern '/dist/*'",
    "build": "node script/build.js",
    "prepublishOnly": "npm run build",
    "postinstall": "npx husky"
  },
  "bin": {
    "gitip": "./dist/index.js"
  },
  "keywords": [
    "git",
    "issue",
    "pull request"
  ],
  "author": {
    "name": "devAnderson",
    "email": "chltjdrhd777@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.11.30",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.29.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.5",
    "@inquirer/select": "^2.3.3",
    "@opentf/cli-pbar": "^0.7.2",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^3.0.0",
    "dotenv": "^16.4.5",
    "enquirer": "^2.4.1",
    "node-fetch": "^3.3.2",
    "tsc-alias": "^1.8.8"
  }
}
